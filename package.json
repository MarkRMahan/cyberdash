{
  "name": "cyberdash",
  "version": "0.0.1",
  "description": "A dashboard to help GMs running Cyberpunk TTRPGs",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "image-map-resizer": "^1.0.10",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "mongoose": "^5.11.10",
    "mongoose-paginate-v2": "^1.3.13",
    "node-ip": "^0.1.2",
    "nodemon": "^2.0.6",
    "react-img-mapper": "^1.1.5"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": "3.0.0"
  },
  "scripts": {
    "client": "cd react-client && npm start",
    "server": "nodemon server.js",
    "debug_server": "nodemon --inspect server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "debug": "concurrently --kill-others-on-fail \"npm run debug_server\" \"npm run client\""
  },
  "main": "server.js",
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkRMahan/cyberdash.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb"
  ],
