  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\server.js"
    },
    {
      "name": "Debug React",
      "webRoot": "${workspaceFolder}/react-client",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:5555"
    }
  ]
}